# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/coffee_app/log/*
!/log/.keep
/coffee_app/tmp

# In case secrets are used
/coffee_app/config/secrets.yml

# Ignore RubyMine config files
.idea/*
